#include <stdio.h>

int main() {
    int n, i;
    float marks[10], total = 0, avg, high, low;
    int pass = 0, fail = 0;
    int Aplus=0, A=0, B=0, C=0, D=0, F=0;

    printf("Enter number of students (max 10): ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%f", &marks[i]);
        if (marks[i] < 0 || marks[i] > 100) { i--; continue; }
    }

    high = low = marks[0];
    for (i = 0; i < n; i++) {
        total += marks[i];
        if (marks[i] > high) high = marks[i];
        if (marks[i] < low) low = marks[i];

        if (marks[i] >= 90) { Aplus++; pass++; }
        else if (marks[i] >= 80) { A++; pass++; }
        else if (marks[i] >= 70) { B++; pass++; }
        else if (marks[i] >= 60) { C++; pass++; }
        else if (marks[i] >= 50) { D++; pass++; }
        else { F++; fail++; }
    }

    avg = total / n;

    printf("\nAverage: %.2f\nHighest: %.2f\nLowest: %.2f\n", avg, high, low);
    printf("Passed: %d  Failed: %d\n", pass, fail);
    printf("A+: %d  A: %d  B: %d  C: %d  D: %d  F: %d\n", Aplus, A, B, C, D, F);

    return 0;
}
